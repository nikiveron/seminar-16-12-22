#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "ru");
	int n, a;
	cout << "Введите кол-во эл-тов массива:" << endl;
	cin >> n;
	int* m = new int[n];
	cout << "Ведите эл-ты массива:" << endl;
	for (int i = 0; i < n; i++) {
		cin >> a;
		m[i] = a;
	}
	m[n - 1] = -1;
	m[n - 2] = -1;
	m[n - 3] = -1;
	for (int k = 0; k < n; k++) cout << m[k] << " ";
	
}
